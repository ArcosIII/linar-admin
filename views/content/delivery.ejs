<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Deliveries</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="../dashboard">Home</a></li>
              <li class="breadcrumb-item active">Deliveries</li>
              <li class="breadcrumb-item active">Show All</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">

            <div class="card">
              <div class="card-header">
                <h3 class="card-title">List of Delivery</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <table id="example1" class="table table-bordered table-striped">
                  <thead>
                  <tr>
                    <th>Delivery No</th>
                    <th>Order No</th>
                    <th>Date Received</th>
                    <th>Client</th>
                    <th>Agent</th>
                    <th>Date Received</th>
                    <th>Qty Delivered</th>
                    <th>Total Price</th>
                    <th>Received By</th>
                  </tr>        
                </thead>
                  <tbody>

                  <% delivery.forEach(function(delivery){%>
                  <tr>
                    <td><%=delivery.delivery_no %></td>
                    <td><%=delivery.purchase_order_no %></td>
                    <td><%=delivery.order_date_received %></td>
                    <td><%=delivery.client_name %></td>
                    <td><%=delivery.agent_name %></td>
                    <td><%=delivery.order_date_received %></td>
                    <td><%=delivery.qty_delivered %></td>
                    <td><%=delivery.total_price %></td>
                    <td><%=delivery.delivery_received_by %></td>
                  </tr>
                  <%})%>
                  </tbody>
                  <tfoot>
                  <tr>
                    <th>Delivery No</th>
                    <th>Order No</th>
                    <th>Date Received</th>
                    <th>Client</th>
                    <th>Agent</th>
                    <th>Date Received</th>
                    <th>Qty Delivered</th>
                    <th>Total Price</th>
                    <th>Received By</th>
                  </tr>
                  </tfoot>
                </table>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>