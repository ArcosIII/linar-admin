<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Orders</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="../dashboard">Home</a></li>
              <li class="breadcrumb-item active">Orders</li>
              <li class="breadcrumb-item active">Show All</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">

            <div class="card">
              <div class="card-header">
                <h3 class="card-title">List of Orders</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <table id="example1" class="table table-bordered table-striped">
                  <thead>
                  <tr>
                    <th>Order ID</th>
                    <th>Item ID</th>
                    <th>Quantity</th>
                    <th>Total Price</th>
                    <th>Ordered By:</th>
                    <th>Agent ID:</th>
                    <th>Order Date</th>
                    <th>Status</th>
                  </tr>        
                </thead>
                  <tbody>
                    
                <% order.forEach(function(order){%>
                  <tr>
                    <td><%=order.id %></td>
                    <td><%=order.item_id %> </td>
                    <td><%=order.total_qty %></td>
                    <td><%=order.total_price %></td>
                    <td><%=order.client_name %></td>
                    <td><%=order.agent_id %></td>
                    <td><%=order.purchase_date %></td>
                    <td><%=order.purchase_status %></td>
                  </tr>
                <%})%>
                  </tbody>
                  <tfoot>
                  <tr>
                    <th>Order ID</th>
                    <th>Item ID</th>
                    <th>Quantity</th>
                    <th>Total Price</th>
                    <th>Ordered By:</th>
                    <th>Agent ID:</th>
                    <th>Order Date</th>
                    <th>Status</th>
                  </tr>
                  </tfoot>
                </table>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>